<#@ template debug="false" hostspecific="true" language="C#" #>
<#+ void AddPublicationScript(NugetContext context){#>
dotnet pack /p:NuspecFile=<#=context.NuspecName#>.nuspec
dotnet nuget push <#="bin\\Debug\\" + context.PackageName#>.<#=context.Version#>.nupkg -s <#=context.PackageSource#>
<#+}#>
